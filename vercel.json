{
    "framework": "vite",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/predict",
            "destination": "/server/app.py"
        },
        {
            "source": "/visitor-count",
            "destination": "/server/app.py"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "server/app.py": {
            "maxDuration": 10
        }
    }
}